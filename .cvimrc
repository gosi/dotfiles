let mapleader = ","
let searchalias g = "google"
set noautofocus
let barposition = "bottom"

map K previousTab
map J nextTab

map ; openCommandBar
map <C-b> :bookmarks<CR>
map <C-w> :quit<CR>

" insert maps
iunmapAll
imap <C-l> forwardChar
imap <C-h> backwardChar
imap <C-f> forwardWord
imap <C-b> backwardWord
imap <C-o> editWithVim
imap <C-j> forwardLine
imap <C-k> backwardLine

map <Leader>b :buffer<Space>
map <Leader>h :history<Space>
map <Leader>th :tabhistory<Space>

map <Leader>gh :open https://github.com<CR>
map <Leader>tw :open https://twitch.tv/following<CR>
map <Leader>yt :open https://www.youtube.com/feed/subscriptions<CR>
<	move current tab left
